integrator              = md
constraints             = h-bonds
constraint-algorithm    = lincs
dt                      = 0.002
nsteps                  = 50000000
nstcomm                 = 100

pbc                     = xyz 
periodic-molecules      = no
rlist                   = 1.2 
nstlist                 = 10
ns-type                 = grid

coulombtype             = PME 
rcoulomb                = 1.2 
cutoff-scheme           = verlet
vdwtype                 = cut-off
vdw-modifier            = force-switch
rvdw-switch             = 1.0
rvdw                    = 1.2 

nstlog                  = 100000
nstenergy               = 5000
nstxout-compressed      = 5000

Tcoupl                  =  nose-hoover
tc-grps                 =  LAUP_LAU  TIP3_POT
tau_t                   =  1.0   1.0
ref_t                   =  305.1 305.1
pcoupl                  =  parrinello-rahman
Pcoupltype              =  semiisotropic
tau_p                   =  2.0
compressibility         =  4.2e-5  4.2e-5
ref_p                   =  1.01325 1.01325

gen-vel                 = no
